<launch>
  <node name="whipbot_main"   pkg="whipbot" type="whipbot_main.py" />
  <node name="get_sernsor" pkg="whipbot" type="get_sernsor.py" output="screen"/>
  <node name="motion_generator"   pkg="whipbot" type="motion_generator.py" output="screen"/>
  <node name="motor_control"   pkg="whipbot" type="motor_control.py"/>

  <node name="torque_control_left"   pkg="kondo_b3mservo_rosdriver" type="torque_control.py" output="screen">
    <param name="/servo_id" value="4"/>
    <remap from="servo_command" to="servo_command_left"/>
    <remap from="servo_info" to="servo_info_left"/>
  </node>
  <node name="torque_control_right"   pkg="kondo_b3mservo_rosdriver" type="torque_control.py" output="screen">
    <param name="/servo_id" value="5"/>
    <remap from="servo_command" to="servo_command_right"/>
    <remap from="servo_info" to="servo_info_right"/>
  </node>

  <node name="joy_node"   pkg="joy" type="joy_node">
    <param name="autorepeat_rate" value="10"/>
  </node>
</launch>
